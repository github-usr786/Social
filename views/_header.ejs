<header>
        Page Header

    <ul>
        <% if (locals.user){ %>
            <li>
                <a href="/users/profile/<%= locals.user.id%>">
                    <%= user.name %>
                </a>
            </li>
            <li>
                <a href="/users/signout">Sign Out</a>
            </li>
        <% }else{ %>

            <li>
                <a href="/users/signin">Sign In</a>
            </li>

            <li>
                <a href="/users/signup">Sign Up</a>
            </li>


        <% } %>
    </ul>

</header>